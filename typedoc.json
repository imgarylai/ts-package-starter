{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "theme": "default",
  "name": "ts-package-starter",
  "readme": "README.md",
  "includeVersion": true,
  "gitRevision": "main",
  "searchInComments": true,
  "validation": {
    "invalidLink": true,
    "notExported": true
  }
}
